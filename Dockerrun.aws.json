{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
     {
        "name": "client",
        "image": "docker1668/multi-client",
        "hostname": "client",
        "essential": false,
        "memory": 128
     },
     {
        "name": "server",
        "image": "docker1668/multi-server",
        "hostname": "api",
        "essential": false,
        "memory": 128
     } ,
     {
        "name": "worker",
        "image": "docker1668/multi-worker",
        "hostname": "worker",
        "essential": false,
        "memory": 128
     },
     {
        "name": "nginx",
        "image": "docker1668/multi-nginx",
        "essential": true,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort":80
          }
        ],
        "links": ["client","server"],
        "memory": 128
     } 
  ]
}