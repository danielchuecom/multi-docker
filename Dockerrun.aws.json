{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
     {
        "name": "client",
        "image": "docker1668/multi-client"
        "hostname": "client",
        "essential": false
     },
     {
        "name": "server",
        "image": "docker1668/multi-server"
        "hostname": "api",
        "essential": false
     } ,
     {
        "name": "worker",
        "image": "docker1668/multi-worker"
        "hostname": "worker",
        "essential": false
     },
     {
        "name": "nginx",
        "image": "docker1668/multi-nginx"
        "essential": true,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort":80
          }
        ],
        "links": ["client","server"]
     } 
  ]
}